{
  "name": "@emergent/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next-typesafe-url && next build",
    "clean": "rimraf .next .turbo",
    "dev": "next-typesafe-url && next dev --port 3000",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,json,md}\"",
    "lint": "eslint . --cache --cache-location node_modules/.cache/eslint/",
    "lint:fix": "eslint . --fix --cache --cache-location node_modules/.cache/eslint/",
    "routes": "next-typesafe-url",
    "routes:watch": "next-typesafe-url --watch",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@emergent/shared": "workspace:*",
    "@lukemorales/query-key-factory": "^1.3.4",
    "@tanstack/react-form": "^1.28.0",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "next-typesafe-url": "^6.1.0",
    "nuqs": "^2.8.8",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.1.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6",
    "zod-validation-error": "^5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.0",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@types/node": "^25.0.0",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-better-tailwindcss": "^4.2.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.0"
  }
}
